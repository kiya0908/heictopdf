{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/feed",
      "destination": "/feed.xml"
    },
    {
      "source": "/rss",
      "destination": "/feed.xml"
    },
    {
      "source": "/rss.xml",
      "destination": "/feed.xml"
    }
  ]
}